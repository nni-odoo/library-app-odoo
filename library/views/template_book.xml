<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="books" name="Books">
        <t t-call="web.html_container">
            <table class="table">
                <thead>
                    <tr>
                        <td>Name</td>
                        <td>ISBN</td>
                    </tr>
                </thead>
                <tbody>
                    <t t-foreach="books" t-as="book">
                        <tr>
                            <td><span t-esc="book.name"/></td>
                            <td><span t-esc="book.isbn"/></td>
                        </tr>
                    </t>
                </tbody>
            </table>
        </t>
    </template>

</odoo>
